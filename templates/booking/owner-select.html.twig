{% for owner in form.owner %}
    {% set index = owner.vars.value %}{# get array index #}
    {% set entity = form.owner.vars.choices[index].data %}{# get entity object #}
    <div id="booking-owner-button-{{ index }}" class=" col-xs-2 col-sm-4 col-md-1" style="" >
        {{ form_row(owner, {'label':false, 'required':false}) }}{# render checkbox #}
        <p class="text-center">{{ entity.name }}</p>
    </div>
{% endfor %}
